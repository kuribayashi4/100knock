    "N = int(input('N=')) #行数Nを受け取る\n",
    "with open('hightemp.txt', 'r') as f:\n",
    "    data = f.readlines()\n",
    "    if N < 0 or N > len(data):\n",
    "        print(\"please input a number between 0 and 24. \") #Nの値が適切か判断\n",
    "    else:\n",
    "        print(''.join(data[-N:])) #末尾のN行を表示"
