   "source": [
    "from collections import Counter\n",
    "s = []\n",
    "with open('hightemp.txt', 'r') as f:\n",
    "    for data in f.readlines():\n",
    "        s.append(data.split()[0])　#1列目のデータをリストsに追加\n",
    "counter = Counter(s)\n",
    "for word, cnt in counter.most_common(): #出現頻度のカウントと並び替え\n",
    "    print(word, cnt)\n",
    "\n"
   ]
